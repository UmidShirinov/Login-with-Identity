<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/jquery-validate/jquery.validate.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.js"></script>
    <link href="~/lib/twitter-bootstrap/css/bootstrap.css" rel="stylesheet" />


</head>
<body>




    <nav class=" col-md-4 offset-md-4 navbar navbar-expand-lg navbar-light bg-light">
        <div class=" container-fluid">
            <a class=" navbar-brand" href="#">BALABALA.com</a>
            <button class=" navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class=" mt-3 collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active"  asp-action="Index">Home</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-item  nav-link" asp-action="SignUp"  asp-controller="Home">Qeydiyyat</a>
                    </li>
                    <li class="nav-item">
                        <a class=" nav-item nav-link" asp-action="Login">Giris</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link disabled" href="#" tabindex="-1" aria-disabled="true">Disabled</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div>
        @RenderBody()
    </div>
</body>
</html>
